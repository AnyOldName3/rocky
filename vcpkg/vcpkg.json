{
    "name": "rocky",
    "version-string": "0.1",
    "port-version": 1,
    "description": "Rocky by Pelican Mapping",
    "homepage": "https://github.com/pelicanmapping/rocky",
    "supports": "!(x86 | wasm32)",
    "dependencies": [
        "cpp-httplib",
        "proj",
        "entt",
        { "name": "gdal", 
          "default-features": false, 
          "features": [ "recommended-features" ] },
        "glm",
        "nlohmann-json",
        "openssl",
        "qt5",
        "spdlog",
        "sqlite3",
        "tinyxml",
        "vsg",
        { "name" : "vsgxchange",
          "features": [ "assimp", "freetype" ] },
        "vsgimgui",
        "vsgqt",
        "zlib"
    ]
}
